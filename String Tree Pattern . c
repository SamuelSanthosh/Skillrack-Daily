#include<stdio.h>
#include <stdlib.h>
int main()
{
    char S[100];
    int i,j;
    scanf("%s",S);
    for(i=0;i<2*strlen(S)-1;i++)
    {
        for(j=0;j<=i;j++)
        {
            if(i%2==0)
                printf("%c",S[i/2]);
            else
                printf("*");
        }
        printf("\n");
    }
}
